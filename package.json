{
  "name": "maokun-explorer",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/carpiediem/maokun-explorer.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "prestart": "npm run geojson",
    "build": "react-scripts build",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test",
    "eject": "react-scripts eject",
    "docs": "jsdoc -c jsdoc.conf.json",
    "geojson": "node ./src/cli/build-geojson.js",
    "tiles": "node ./src/cli/download-tiles.js",
    "wiki": "node ./src/cli/wikipedia-list.js",
    "extract-intl": "extract-messages -l=en,zh -o src/translations -d en --flat false 'src/**/!(*.test).js'"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "d3-geo": "^2.0.1",
    "d3-selection": "^2.0.0",
    "fast-deep-equal": "^3.1.3",
    "leaflet": "^1.7.1",
    "node-fetch": "^2.6.1",
    "react": "^16.14.0",
    "react-intl": "^5.8.8",
    "react-katex": "^2.0.2",
    "react-leaflet": "^2.8.0",
    "react-scripts": "^4.0.0",
    "react-split-pane": "^0.1.92",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "better-docs": "^2.3.2",
    "cli-progress": "^3.8.2",
    "cross-env": "^7.0.2",
    "css-mediaquery": "^0.1.2",
    "csv-parse": "^4.15.4",
    "extract-react-intl-messages": "4.1.1",
    "full-icu": "^1.3.1",
    "gm": "^1.23.1",
    "jest-expect-message": "^1.0.2",
    "jsdoc": "^3.6.6",
    "prop-types": "^15.7.2",
    "react-dom": "^17.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
